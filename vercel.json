{
  "version": 2,
  "builds": [
    {
      "src": "apps/web/package.json",
      "use": "@vercel/static-build"
    },
    {
      "src": "apps/api/src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "apps/api"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/index.html"
    }
  ]
}
